                  <div class="col-md-6 col-sm-6 ">
                    <input type="text" id="coachemail" name="coachemail" required="required" class="form-group " placeholder="Email of the Coach">
                  </div>
                  </div>
                </div>

                  <div class="item form-group">
                    <label class="col-form-label col-md-3 col-sm-3 label-align" for="coach">T-Shirt size of the coach <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 ">
                      <select class="form-control" id="tshirtcoach" name="tshirtcoach">
                        <option disabled>... Choose Option ...</option>
                        <option value="M">Medium</option>
                        <option value="L">Large</option>
                        <option value="XL">Extra Large</option>
                      </select>
                    </div>
                  </div>
                
              </div>


