              <form method="POST" action="/ProgrammingContest/register" id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">
  
                <div class="row">
                <div class="item form-group">
                  <div class="col-md-6 col-sm-6 ">
                    <input type="text" id="teamname" name="teamname" required="required" class="form-group " placeholder="Team Name">
                  </div>
                </div>
  
                <div class="item form-group">
                  <div class="col-md-6 col-sm-6 ">
                    <input type="text" id="institutename" name="institutename" required="required" class="form-group " placeholder="Institution Name">
                  </div>
