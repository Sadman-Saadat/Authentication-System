<div class="right_col" role="main">
    <!-- top tiles -->
    <div class="col-md-12 col-sm-12">
      <div class="x_panel">
        <div class="x_title">
          <h2>Programming Contest<small>Team List</small></h2>
          <ul class="nav navbar-right panel_toolbox">
            <li>
              <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
          </ul>
          <div class="clearfix"></div>
        </div>
        <div class="x_content">
            <br />
        <% let success= 'Team deleted successfully!'%> 
        <% let success2= 'Team has been selected succesfully!'%> 
        <% if (error.length>0) {if
        (error==success||success2||success3){%>
        <div class="alert alert-success"><%=error%></div>
        <%}else{%>
        <div class="alert alert-danger"><%=error%></div>
        <%}%> <%}%>

          <div class="row">
            <div class="col-sm-12">
              <div class="card-box table-responsive">
                <table
                  id="datatable-responsive"
                  class="table table-striped table-bordered nowrap"
                  cellspacing="0"
                  width="100%"
                >
                  <thead>
                    <tr>
                        <th>
                            Team Name
                        </th>
                      <th>Institute</th>
                      <th>
                        Coach Name
                      </th>
                      <th>
                        Coach Contact
                      </th>
                      <th>
                        Team Leader Name
                      </th>
                      <th>
                        Team Leader Contact
                      </th>
                      <th>
                        Member-1 Name
                      </th>
                      <th>
                        Member-2 Name
                      </th>
                      <th>Total</th>
                      <th>Paid</th>
                      <th>Selected</th>
                      <th>
                        Payment<br />
                        Status
                      </th>
                      <th>Select</th>
                      <th>
                        Payment<br />
                        Done
                      </th>
                      <th>Edit</th>
                      <th>Delete</th>
                      <th>confirmCode</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%if(teams.length>0){
                    teams.forEach(team=>{%>
                    <tr>
                      <td><%=team.teamname%></td>
                      <td><%=team.institutename%></td>
                      <td><%=team.coachname%></td>
                      <td><%=team.coachcontact%> <br /> <%=team.coachmail%></td>
                      <td><%=team.teamleadername%></td>
                      <td><%=team.teamleadercontact%> <br /><%=team.teamleaderemail%></td>
                      <td><%=team.member1name%></td>
                      <td><%=team.member2name%></td>
                      <td><%=team.total%></td>
                      <td><%=team.paid%></td>
                      
                      <td>
                        <%if(team.selected==true){%>
                        <p style="color: green">Yes</p>
                        <%}else{%>
                        <p style="color: brown">No</p>
                        <%}%>
                      </td>
                      <td>
                        <%if(team.paid==team.total){%>
                        <p style="color: green">Successful</p>
                        <%}else{%>
                        <p style="color: brown">Incomplete</p>
                        <%}%>
                      </td>
                      <td>
                        <% const selectURL= 'select/'+ team._id %>
                        <a href="<%= selectURL %>"
                          ><i class="fa fa-check"></i
                          ><span class="fa fa-chevron"></span
                        ></a>
                      </td>
                      <td>
                        <% const paymentDoneURL= 'paymentDone/'+ team._id
                        %>
                        <a href="<%= paymentDoneURL %>"
                          ><i class="fa fa-money"></i
                          ><span class="fa fa-chevron"></span
                        ></a>
                      </td>
                      <td>
                        <% const editURL= 'editParticipant/'+team._id %>
                        <a href="<%= editURL%>" 
                          ><i class="fa fa-pencil"></i
                          ><span class="fa fa-chevron"></span
                        ></a>
                      </td>
                      
                      <td>
                        <% const deleteURL= 'delete/'+ team._id %>
                        <a href="<%= deleteURL%>"
                          ><i class="fa fa-trash"></i
                          ><span class="fa fa-chevron"></span
                        ></a>
                      </td>

                      <td><%=team.confirmCode%></td>

                    </tr>
                    <% }) }%>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>